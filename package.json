    "@wdio/spec-reporter": "^8.39.0",
    "ctrf": "^0.0.9",
    "eslint": "^8.57.0",
    "fantasticon": "^3.0.0",
    "github-actions-ctrf": "^0.0.20",
    "glob": "^11.0.0",
    "junit-to-ctrf": "^0.0.4",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2",
