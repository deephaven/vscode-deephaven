{
  "name": "vscode-deephaven",
  "version": "0.1.4",
  "displayName": "Deephaven in VS Code",
  "description": "",
  "publisher": "deephaven",
  "icon": "images/dh-community-on-dark-128.png",
  "galleryBanner": {
    "color": "#040427",
    "theme": "dark"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/deephaven/vscode-deephaven"
  },
  "engines": {
    "vscode": "^1.91.0"
  },
  "categories": [
    "Data Science",
    "Snippets"
  ],
  "activationEvents": [
    "onFileSystem:dhfs",
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "Deephaven in VS Code",
      "properties": {
        "vscode-deephaven.core-servers": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": [
            "http://localhost:10000/"
          ],
          "description": "Deephaven Core servers"
        }
      }
    },
    "snippets": [
      {
        "language": "python",
        "path": "./snippets.code-snippets"
      }
    ],
    "commands": [
      {
        "command": "vscode-deephaven.runCode",
        "title": "Run Deephaven File",
        "icon": "$(run-all)"
      },
      {
        "command": "vscode-deephaven.runSelection",
        "title": "Run Deephaven Selected Lines",
        "icon": "$(run)"
      },
      {
        "command": "vscode-deephaven.selectConnection",
        "title": "Deephaven: Select Connection"
      },
      {
        "command": "vscode-deephaven.downloadLogs",
        "title": "Deephaven: Download Logs"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "vscode-deephaven.runCode",
          "when": "false"
        },
        {
          "command": "vscode-deephaven.runSelection",
          "when": "false"
        }
      ],
      "editor/title/run": [
        {
          "command": "vscode-deephaven.runCode",
          "group": "navigation",
          "when": "editorLangId == python"
        },
        {
          "command": "vscode-deephaven.runSelection",
          "group": "navigation",
          "when": "editorLangId == python"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "clean": "rm -rf out",
    "compile": "npm run clean && tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "package": "vsce package -o releases/ && vsce package -o releases/vscode-deephaven-latest.vsix",
    "publish:pre": "vsce publish --pre-release",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "20.14.10",
    "@types/vscode": "^1.91.0",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "@vscode/test-electron": "^2.4.1",
    "@vscode/vsce": "^2.30.0",
    "eslint": "^8.57.0",
    "glob": "^11.0.0",
    "mocha": "^10.6.0",
    "typescript": "^5.5.3"
  },
  "dependencies": {
    "ws": "^8.18.0"
  }
}
